
PATH="$HOME/bin:$HOME/.local/bin:$PATH"
PATH="$HOME/Projects/mercurial/contrib/:$PATH"
export WORKON_HOME=$HOME/.local/virtualenvs

export PATH="$WORKON_HOME/tools/bin:$PATH"
export AUTOSSH_POLL=30

export PKG_CONFIG_PATH=$HOME/.local/lib/pkgconfig
export LD_LIBRARY_PATH=$HOME/.local/lib


zstyle ':vcs_info:*' disable bzr # disable bzr 

for file in $S_SCRIPTDIR/functions/*
do
    source $file
done

unset file

. $S_SCRIPTDIR/aliases

